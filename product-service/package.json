    "esbuild": "^0.14.11",
    "jest": "^29.4.3",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
